[31m-QUnit.jsDump = (function() {[m
[31m-	function quote( str ) {[m
[31m-		return '"' + str.toString().replace(/"/g, '\\"') + '"';[m
[31m-	};[m
[31m-	function literal( o ) {[m
[31m-		return o + '';[m
[31m-	};[m
[31m-	function join( pre, arr, post ) {[m
[31m-		var s = jsDump.separator(),[m
[31m-			base = jsDump.indent(),[m
[31m-			inner = jsDump.indent(1);[m
