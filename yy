[31m-                </ul>[m
[31m-              </div>[m
[31m-              <input type="text">[m
[31m-            </div>[m
[31m-            <div class="input-append">[m
[31m-              <input type="text">[m
[31m-              <div class="btn-group">[m
[31m-                <button class="btn" tabindex="-1">Action</button>[m
[31m-                <button class="btn dropdown-toggle" data-toggle="dropdown" tabindex="-1">[m
[31m-                  <span class="caret"></span>[m
[31m-                </button>[m
[31m-                <ul class="dropdown-menu">[m
[31m-                  <li><a href="#">Action</a></li>[m
