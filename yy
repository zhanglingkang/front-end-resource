[31m-[m
[31m-	var reName = /^function (\w+)/;[m
[31m-[m
[31m-	var jsDump = {[m
[31m-		parse:function( obj, type, stack ) { //type is used mostly internally, you can fix a (custom)type in advance[m
[31m-			stack = stack || [ ];[m
[31m-			var parser = this.parsers[ type || this.typeOf(obj) ];[m
[31m-			type = typeof parser;[m
[31m-			var inStack = inArray(obj, stack);[m
[31m-			if (inStack != -1) {[m
[31m-				return 'recursion('+(inStack - stack.length)+')';[m
[31m-			}[m
