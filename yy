[31m-&lt;script&gt;[m
[31m-  $(function () {[m
[31m-    $('#myTab a:last').tab('show');[m
[31m-  })[m
[31m-&lt;/script&gt;[m
[31m-</pre>[m
[31m-[m
[31m-          <h3>{{_i}}Events{{/i}}</h3>[m
[31m-          <table class="table table-bordered table-striped">[m
[31m-            <thead>[m
[31m-             <tr>[m
[31m-               <th style="width: 150px;">{{_i}}Event{{/i}}</th>[m
[31m-               <th>{{_i}}Description{{/i}}</th>[m
[31m-             </tr>[m
[31m-            </thead>[m
