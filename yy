[31m-			return (type == 'string') ? parser : this.parsers.error;[m
[31m-		},[m
[31m-		typeOf:function( obj ) {[m
[31m-			var type;[m
[31m-			if ( obj === null ) {[m
[31m-				type = "null";[m
[31m-			} else if (typeof obj === "undefined") {[m
[31m-				type = "undefined";[m
[31m-			} else if (QUnit.is("RegExp", obj)) {[m
[31m-				type = "regexp";[m
[31m-			} else if (QUnit.is("Date", obj)) {[m
[31m-				type = "date";[m
[31m-			} else if (QUnit.is("Function", obj)) {[m
[31m-				type = "function";[m
[31m-			} else if (typeof obj.setInterval !== undefined && typeof obj.document !== "undefined" && typeof obj.nodeType === "undefined") {[m
[31m-				type = "window";[m
[31m-			} else if (obj.nodeType === 9) {[m
[31m-				type = "document";[m
[31m-			} else if (obj.nodeType) {[m
[31m-				type = "node";[m
[31m-			} else if (typeof obj === "object" && typeof obj.length === "number" && obj.length >= 0) {[m
[31m-				type = "array";[m
[31m-			} else {[m
[31m-				type = typeof obj;[m
[31m-			}[m
[31m-			return type;[m
[31m-		},[m
[31m-		separator:function() {[m
[31m-			return this.multiline ?	this.HTML ? '<br />' : '\n' : this.HTML ? '&nbsp;' : ' ';[m
[31m-		},[m
[31m-		indent:function( extra ) {// extra can be a number, shortcut for increasing-calling-decreasing[m
[31m-			if ( !this.multiline )[m
[31m-				return '';[m
[31m-			var chr = this.indentChar;[m
[31m-			if ( this.HTML )[m
[31m-				chr = chr.replace(/\t/g,'   ').replace(/ /g,'&nbsp;');[m
[31m-			return Array( this._depth_ + (extra||0) ).join(chr);[m
[31m-		},[m
[31m-		up:function( a ) {[m
[31m-			this._depth_ += a || 1;[m
[31m-		},[m
[31m-		down:function( a ) {[m
[31m-			this._depth_ -= a || 1;[m
[31m-		},[m
[31m-		setParser:function( name, parser ) {[m
[31m-			this.parsers[name] = parser;[m
[31m-		},[m
[31m-		// The next 3 are exposed so you can use them[m
[31m-		quote:quote,[m
[31m-		literal:literal,[m
[31m-		join:join,[m
[31m-		//[m
[31m-		_depth_: 1,[m
[31m-		// This is the list of parsers, to modify them, use jsDump.setParser[m
[31m-		parsers:{[m
[31m-			window: '[Window]',[m
[31m-			document: '[Document]',[m
[31m-			error:'[ERROR]', //when no parser is found, shouldn't happen[m
[31m-			unknown: '[Unknown]',[m
[31m-			'null':'null',[m
[31m-			'undefined':'undefined',[m
[31m-			'function':function( fn ) {[m
[31m-				var ret = 'function',[m
[31m-					name = 'name' in fn ? fn.name : (reName.exec(fn)||[])[1];//functions never have name in IE[m
